doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    title Mikkkkkkkkkkkkkkkke - A smart person you enjoy work with
    meta(name='viewport', content='width=device-width, initial-scale=1')
    link(rel='stylesheet', type='text/css', media='screen', href='main.css')
    //- link(rel='stylesheet', type='text/css', media='screen', href='loader.css')
    link(rel='stylesheet', type='text/css', media='screen', href='flex.css')
    link(rel='stylesheet', type='text/css', media='screen', href='form.css')
    link(href='https://fonts.googleapis.com/css?family=Montserrat|Raleway|Finger+Paint', rel='stylesheet')
    link(href='https://unpkg.com/aos@next/dist/aos.css', rel='stylesheet')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/typed.js/2.0.8/typed.min.js')
    //- https://github.com/wvengen/d3-wordcloud
    script(src="https://cdnjs.cloudflare.com/ajax/libs/animejs/2.2.0/anime.min.js")
    //- script(src="https://cdnjs.cloudflare.com/ajax/libs/vivus/0.4.4/vivus.min.js")
    meta(property='og:title', content='Mike - A smart person you enjoy work with')
    meta(property='og:type', content='profile')
    meta(property='og:url', content='https://namike.me')
    meta(property='og:image', content='./images/me.jpg')

  body
    .border
      .onesec.d--f.ai--c.fd--c.jc--c(style="height: 80vh; position: relative")
        img.down-arrow(data-aos="fade-down", data-aos-offset="-100" style="position:absolute; bottom: 0", src="data:image/svg+xml;utf8;base64,")
        div
          h1(data-wrap='h1', style="font-weight: normal")
            | HI, I am Mike, a 
            div.typing-warp
              span.typing
      //- skills
      center(style="margin-bottom: 40px;", data-aos="fade-up")
        h3(style="margin-top: 40px; margin-bottom: 32px") I KNOW
        #my_canvas(style="width: 80vw; height: 50vh")
      //- works
      .d--f.jc--c.ai--c.fd--c(style="padding: 16px 32px;flex: 1;")
        .d--f
          h3(style="margin-top: 40px; margin-bottom: 32px") MY WORKS
        .sc-grid
          each val,index in sc
            .card(id="card"+index, data-aos="fade-up", data-aos-delay=`${index * 200}`)
              div.d--f.fd--c
                .img(style="background-image: url("+val.img+"); width: 80%; margin: 8px auto;")
                .container(style="flex:")
                  div(style="margin-bottom: 8px; font-weight: bold")= val.name
                  div(style="opacity: 0.7")= val.desc
      //- contact
      center(data-aos="fade-right")
        h3(style="margin-top: 40px; margin-bottom: 32px") LOOK FOR HELP?
        form#form.topBefore(netlify, name="contact")
          input#name(name="name", type='text', placeholder='NAME')
          input#email(name="email", type='text', placeholder='E-MAIL')
          textarea#message(name="message", type='text', placeholder='MESSAGE')
          input#submit(type='submit', value='FIND MIKE FOR BETTER LIFE')

      div(style="height: 999px; width: 100vw")
        a-scene(embedded='', arjs='sourceType: webcam;')
          a-box(position='0 0.5 0', material='opacity: 0.5;')
          a-marker-camera(preset='hiro')

    script(src='main.js')
    //- aos
    script(src="https://unpkg.com/aos@next/dist/aos.js")
    script.
      if ("serviceWorker" in navigator) {
        navigator.serviceWorker
          .register("sw.js")
          .then(function(registration) {
            console.log(
              "Service Worker registration successful with scope: ",
              registration.scope
            );
          })
          .catch(function(err) {
            console.log("Service Worker registration failed: ", err);
          });
      }
    script.
      AOS.init();
